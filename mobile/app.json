{
  "expo": {
    "name": "sacolafacil",
    "slug": "sacolafacil",
    "version": "1.0.0",
    "scheme": "sacolafacil",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-secure-store"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.skrodrigo.sacolafacil"
    },
    "extra": {
      "eas": {
        "projectId": "e7e4861a-2276-44d0-8655-30a38e4cb80f"
      }
    }
  }
}
